<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
           	<pattern>
				%d{HH:mm:ss.SSS} [%thread] %-5level %logger{32} %class{26} -
				%msg%n
			</pattern>
        </encoder>
    </appender>
    <root level="info">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
